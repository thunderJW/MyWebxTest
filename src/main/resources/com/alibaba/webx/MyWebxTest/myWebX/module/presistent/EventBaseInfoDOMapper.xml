<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.alibaba.webx.MyWebxTest.myWebX.module.presistent.EventBaseInfoDOMapper" >
	<resultMap type="eventBaseInfoDo" id="eventBaseInfoDOResultMap">
		<id property="id" column="id" />
		<result property="title" column="title" />
		<result property="eventType" column="type" />
		<result property="brandId" column="brand_id" />
	</resultMap>

	<insert id="createEvent" parameterType="eventBaseInfoDo">
		insert into event_base_info (title,type,brand_id) values (#{title},#{eventType},#{brandId})
	</insert>
	
	<select id="getAllEvents" resultMap="eventBaseInfoDOResultMap">
		select id, title, type, brand_id from event_base_info 
	</select>
</mapper>